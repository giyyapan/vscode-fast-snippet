{
    "name": "fast-snippet",
    "displayName": "Fast Snippet",
    "description": "Insert small snippet (like '=>') with one command.",
    "repository": {
        "type": "git",
        "url": "https://github.com/giyyapan/vscode-fast-snippet"
    },
    "version": "1.0.1",
    "publisher": "giyyapan",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:fastsnippet.insertText"
    ],
    "main": "./out/src/extension",
    "contributes": {},
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}